                                </div>
                            </div>

                            <div class="col-md-8">
                                <div class="map-container rounded" style="overflow: hidden; border-radius: 10px;">
                                    <iframe width="500%" height="350px"
                                            frameborder="0" scrolling="no"
                                            marginheight="0" marginwidth="0"
                                            src="https://maps.google.com/maps?width=100%25&amp;height=600&amp;hl=en&amp;q=bd.%20Moscova,
                %20Chisinau+(BookShop)&amp;t=&amp;z=14&amp;ie=UTF8&amp;iwloc=B&amp;output=embed">
                                    </iframe>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Card for Questions and Form -->

            <div class="section-header align-center">
                <div>
                    <h2 class="section-title">Questions</h2>
                </div>
            </div>


            <div class="row justify-content-center">
                <div class="col-lg-8">
                    <div class="card p-4 shadow-sm" style="border-radius: 15px; background-color: #f8f9fa;">
                        <form action="/About/SendMessage" method="POST">
                            <div class="row">
                                <div class="form-group mb-3 col-md-6">
                                    <label for="yourName">Your Name</label>
                                    <input type="text" class="form-control" id="yourName" name="yourName" required>
                                </div>

                                <div class="form-group mb-3 col-md-6">
                                    <label for="yourEmail">Your Email</label>
                                    <input type="email" class="form-control" id="yourEmail" name="yourEmail" required>
                                </div>
                            </div>

                            <div class="form-group mb-3">
                                <label for="subject">Subject</label>
                                <input type="text" class="form-control" id="subject" name="subject">
                            </div>

                            <div class="form-group mb-3">
                                <label for="message">Message</label>
                                <textarea class="form-control" id="message" name="message" rows="4" required></textarea>
                            </div>

                            <button type="submit" class="btn btn-primary w-100 py-2" style="border-radius: 10px;">
                                Send message
                            </button>
                        </form>

                        @if (TempData["SuccessMessage"] != null)
                        {
                            <div class="alert alert-success mt-3">@TempData["SuccessMessage"]</div>
                        }
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

